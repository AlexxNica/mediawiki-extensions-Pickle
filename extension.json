{
	"name": "Spec",
	"version": "0.1.0",
	"author": [
		"John Erling Blad"
	],
	"url": "https://github.com/jeblad/Spec",
	"descriptionmsg": "spec-desc",
	"license-name": "GPL v2",
	"type": "other",
	"AutoloadClasses": {
		"Spec\\Hooks": "includes/Hooks.php",
		"Spec\\Common": "includes/Common.php",
		"Spec\\IIdentifyResultStrategy":
			"includes/IdentifyResult/IIdentifyResultStrategy.php",
		"Spec\\IdentifyResultByPatternStrategy":
			"includes/IdentifyResult/IdentifyResultByPatternStrategy.php",
		"Spec\\IdentifyResultSingleton":
			"includes/IdentifyResult/IdentifyResultSingleton.php"
	},
	"config": {
		"SpecFinalStates": [
			{
				"class": "Spec\\IdentifyResultByPatternStrategy",
				"name": "failed",
				"pattern": "/\\bfailed\\b/"
			},
			{
				"class": "Spec\\IdentifyResultByPatternStrategy",
				"name": "pending",
				"pattern": "/\\bpending\\b/"
			},
			{
				"class": "Spec\\IdentifyResultByPatternStrategy",
				"name": "good",
				"pattern": "/\\bgood\\b/"
			}
		]
	},
	"callback": "Spec\\Hooks::onExtensionLoad",
	"ExtensionMessagesFiles": {
	},
	"Hooks": {
		"ArticleViewHeader": [
			"Spec\\Hooks::onArticleViewHeader"
		],
		"UnitTestsList": [
			"Spec\\Hooks::onUnitTestsList"
		]
	},
	"MessagesDirs": {
		"Spec": [
			"i18n"
		]
	},
	"ResourceModules": {
		"ext.spec.defaultDisplay": {
			"styles": {
				"0": "ext.spec.defaultDisplay.css"
			}
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "modules",
		"remoteExtPath": "Spec/modules"
	},
	"TrackingCategories": [
		"spec-test-category-good",
		"spec-test-category-pending",
		"spec-test-category-missing",
		"spec-test-category-unknown"
	],
	"SpecialPages": {
	},
	"manifest_version": 1
}
