{
	"name": "Spec",
	"version": "0.1.0",
	"author": [
		"John Erling Blad"
	],
	"url": "https://github.com/jeblad/Spec",
	"descriptionmsg": "spec-desc",
	"license-name": "GPL v2",
	"type": "other",
	"AutoloadClasses": {
		"Spec\\Hooks":
			"includes/Hooks.php",
		"Spec\\Common":
			"includes/Common.php",

		"Spec\\ITrackUser":
			"includes/TrackUser/ITrackUser.php",
		"Spec\\TrackUser":
			"includes/TrackUser/TrackUser.php",

		"Spec\\IStrategies":
			"includes/Strategies/IStrategies.php",
		"Spec\\Strategies":
			"includes/Strategies/Strategies.php",

		"Spec\\TNamedStrategy":
			"includes/Strategies/TNamedStrategy.php",
		"Spec\\TNamedStrategies":
			"includes/Strategies/TNamedStrategies.php",

		"Spec\\ITrackLogEntryStrategy":
			"includes/TrackLogEntry/ITrackLogEntryStrategy.php",
		"Spec\\TrackLogEntryBaseStrategy":
			"includes/TrackLogEntry/TrackLogEntryBaseStrategy.php",
		"Spec\\TrackLogEntryDefaultStrategy":
			"includes/TrackLogEntry/TrackLogEntryDefaultStrategy.php",
		"Spec\\TrackLogEntryCommonStrategy":
			"includes/TrackLogEntry/TrackLogEntryCommonStrategy.php",
		"Spec\\TrackLogEntryStrategies":
			"includes/TrackLogEntry/TrackLogEntryStrategies.php",

		"Spec\\ITrackCategoryStrategy":
			"includes/TrackCategory/ITrackCategoryStrategy.php",
		"Spec\\TrackCategoryBaseStrategy":
			"includes/TrackCategory/TrackCategoryBaseStrategy.php",
		"Spec\\TrackCategoryDefaultStrategy":
			"includes/TrackCategory/TrackCategoryDefaultStrategy.php",
		"Spec\\TrackCategoryCommonStrategy":
			"includes/TrackCategory/TrackCategoryCommonStrategy.php",
		"Spec\\TrackCategoryStrategies":
			"includes/TrackCategory/TrackCategoryStrategies.php",

		"Spec\\ITrackIndicatorStrategy":
			"includes/TrackIndicator/ITrackIndicatorStrategy.php",
		"Spec\\TrackIndicatorBaseStrategy":
			"includes/TrackIndicator/TrackIndicatorBaseStrategy.php",
		"Spec\\TrackIndicatorDefaultStrategy":
			"includes/TrackIndicator/TrackIndicatorDefaultStrategy.php",
		"Spec\\TrackIndicatorCommonStrategy":
			"includes/TrackIndicator/TrackIndicatorCommonStrategy.php",
		"Spec\\TrackIndicatorStrategies":
			"includes/TrackIndicator/TrackIndicatorStrategies.php",

		"Spec\\ITestAnythingProtocolStrategy":
			"includes/TestAnythingProtocol/ITestAnythingProtocolStrategy.php",
		"Spec\\TestAnythingProtocolBaseStrategy":
			"includes/TestAnythingProtocol/TestAnythingProtocolBaseStrategy.php",
		"Spec\\TestAnythingProtocolCommonStrategy":
			"includes/TestAnythingProtocol/TestAnythingProtocolCommonStrategy.php",
		"Spec\\TestAnythingProtocol13Strategy":
			"includes/TestAnythingProtocol/TestAnythingProtocol13Strategy.php",
		"Spec\\TestAnythingProtocolStrategies":
			"includes/TestAnythingProtocol/TestAnythingProtocolStrategies.php",

		"Spec\\IExtractStatusStrategy":
			"includes/ExtractStatus/IExtractStatusStrategy.php",
		"Spec\\ExtractStatusDefaultStrategy":
			"includes/ExtractStatus/ExtractStatusDefaultStrategy.php",
		"Spec\\ExtractStatusByPatternStrategy":
			"includes/ExtractStatus/ExtractStatusByPatternStrategy.php",
		"Spec\\ExtractStatusStrategies":
			"includes/ExtractStatus/ExtractStatusStrategies.php",

		"Spec\\IInvokeSubpageStrategy":
			"includes/InvokeSubpage/IInvokeSubpageStrategy.php",
		"Spec\\InvokeSubpageDefaultStrategy":
			"includes/InvokeSubpage/InvokeSubpageDefaultStrategy.php",
		"Spec\\InvokeSubpageByContentTypeStrategy":
			"includes/InvokeSubpage/InvokeSubpageByContentTypeStrategy.php",
		"Spec\\InvokeSubpageStrategies":
			"includes/InvokeSubpage/InvokeSubpageStrategies.php",

		"Spec\\ITestConsoleView":
			"includes/TestConsole/ITestConsoleView.php",
		"Spec\\TestConsoleView":
			"includes/TestConsole/TestConsoleView.php",

		"Spec\\ISubLinksView":
			"includes/SubLinks/ISubLinksView.php",
		"Spec\\SubLinksView":
			"includes/SubLinks/SubLinksView.php",

		"Spec\\LuaLibrary":
			"includes/LuaLibrary/LuaLibrary.php",

		"Spec\\Tests\\StrategiesTestCase":
			"tests/phpunit/includes/Strategies/StrategiesTestCase.php",

		"Spec\\Tests\\TrackLogEntryStrategyTestCase":
			"tests/phpunit/includes/TrackLogEntry/TrackLogEntryStrategyTestCase.php",

		"Spec\\Tests\\TrackCategoryStrategyTestCase":
			"tests/phpunit/includes/TrackCategory/TrackCategoryStrategyTestCase.php",

		"Spec\\Tests\\TestAnythingProtocolStrategyTestCase":
			"tests/phpunit/includes/TestAnythingProtocol/TestAnythingProtocolStrategyTestCase.php",

		"Spec\\Tests\\TrackIndicatorStrategyTestCase":
			"tests/phpunit/includes/TrackIndicator/TrackIndicatorStrategyTestCase.php",

		"Spec\\Tests\\InvokeSubpageStrategyTestCase":
			"tests/phpunit/includes/InvokeSubpage/InvokeSubpageStrategyTestCase.php"
	},
	"config": {
        "_prefix": "wgSpec",
		"TrackUserID": 3,
		"RenderStyles": {
			"compact": "speclib/render/compact",
			"full": "speclib/render/full",
			"vivid": "speclib/render/vivid"
		},
		"RenderTypes": {
			"result": "ResultRenderStrategy",
			"plan": "PlanRenderStrategy"
		},
		"ExtractorStrategy": [
			"speclib/extractor/NilExtractorStrategy",
			"speclib/extractor/FalseExtractorStrategy",
			"speclib/extractor/TrueExtractorStrategy",
			"speclib/extractor/StringExtractorStrategy",
			"speclib/extractor/NumberExtractorStrategy"
		],
		"NeglectPages": [
			"/doc",
			"/conf",
			"/data",
			"/i18n",
			"/l10n"
		],
		"TrackIndicator": [
			{
				"class": "Spec\\TrackIndicatorCommonStrategy",
				"name": "failing"
			},
			{
				"class": "Spec\\TrackIndicatorCommonStrategy",
				"name": "pending"
			},
			{
				"class": "Spec\\TrackIndicatorCommonStrategy",
				"name": "good"
			},
			{
				"class": "Spec\\TrackIndicatorCommonStrategy",
				"name": "exists"
			},
			{
				"class": "Spec\\TrackIndicatorCommonStrategy",
				"name": "missing"
			},
			{
				"class": "Spec\\TrackIndicatorDefaultStrategy"
			}
		],
		"TrackCategory": [
			{
				"class": "Spec\\TrackCategoryCommonStrategy",
				"name": "failing"
			},
			{
				"class": "Spec\\TrackCategoryCommonStrategy",
				"name": "pending"
			},
			{
				"class": "Spec\\TrackCategoryCommonStrategy",
				"name": "good"
			},
			{
				"class": "Spec\\TrackCategoryCommonStrategy",
				"name": "exists"
			},
			{
				"class": "Spec\\TrackCategoryCommonStrategy",
				"name": "missing"
			},
			{
				"class": "Spec\\TrackCategoryDefaultStrategy"
			}
		],
		"TrackLogEntry": [
			{
				"class": "Spec\\TrackLogEntryCommonStrategy",
				"name": "failing"
			},
			{
				"class": "Spec\\TrackLogEntryCommonStrategy",
				"name": "pending"
			},
			{
				"class": "Spec\\TrackLogEntryCommonStrategy",
				"name": "good"
			},
			{
				"class": "Spec\\TrackLogEntryCommonStrategy",
				"name": "exists"
			},
			{
				"class": "Spec\\TrackLogEntryCommonStrategy",
				"name": "missing"
			},
			{
				"class": "Spec\\TrackLogEntryDefaultStrategy"
			}
		],
		"InvokeSubpage": [
			{
				"class": "Spec\\InvokeSubpageByContentTypeStrategy",
				"type": "Scribunto",
				"name": "testspec"
			},
			{
				"class": "Spec\\InvokeSubpageByContentTypeStrategy",
				"type": "Scribunto",
				"name": "testcase"
			},
			{
				"class": "Spec\\InvokeSubpageDefaultStrategy"
			}
		],
		"TestAnythingProtocol": [
			{
				"class": "Spec\\TestAnythingProtocol13Strategy"
			},
			{
				"class": "Spec\\TestAnythingProtocolCommonStrategy"
			}
		],
		"ExtractStatus": [
			{
				"class": "Spec\\ExtractStatusByPatternStrategy",
				"name": "failing",
				"pattern": "/^(fail(ing|ed|)|bad|not\\s+ok|bail(ing|ed|)\\s+out!)$/i"
			},
			{
				"class": "Spec\\ExtractStatusByPatternStrategy",
				"name": "pending",
				"pattern": "/^(pending|todo|skipp?(ing|ed|))$/i"
			},
			{
				"class": "Spec\\ExtractStatusByPatternStrategy",
				"name": "good",
				"pattern": "/^(good|ok)$/i"
			},
			{
				"class": "Spec\\ExtractStatusDefaultStrategy"
			}
		]
	},
	"callback": "Spec\\Hooks::onExtensionSetup",
	"ExtensionMessagesFiles": {
	},
	"Hooks": {
		"SpecTesterGadgets": [
			"Spec\\TrackIndicatorStrategies::addIndicator"
		],
		"SpecTesteeGadgets": [
			"Spec\\TrackIndicatorStrategies::addIndicator",
			"Spec\\TrackCategoryStrategies::addCategorization",
			"Spec\\TrackLogEntryStrategies::addLogEntry"
		],
		"ContentAlterParserOutput": [
			"Spec\\Hooks::onContentAlterParserOutput"
		],
		"OutputPageParserOutput": [
			"Spec\\SubLinksView::onOutputPageParserOutput"
		],
		"EditPageBeforeEditChecks": [
			"Spec\\TestConsoleView::onEditPageBeforeEditChecks"
		],
		"EditPage::showReadOnlyForm:initial": [
			"Spec\\TestConsoleView::onShowReadOnlyFormInitial"
		],
		"ScribuntoExternalLibraries": [
			"Spec\\LuaLibrary::registerScribuntoLibraries"
		],
		"ScribuntoExternalLibraryPaths": [
			"Spec\\LuaLibrary::registerScribuntoExternalLibraryPaths"
		],
		"UnitTestsList": [
			"Spec\\Hooks::onUnitTestsList"
		]
	},
	"MessagesDirs": {
		"Spec": [
			"i18n"
		]
	},
	"ResourceModules": {
		"ext.spec.default": {
			"styles": {
				"0": "ext.spec.default.css"
			}
		},
		"ext.spec.console": {
			"styles": {
				"0": "ext.spec.console.css"
			},
			"scripts": {
				"0": "ext.spec.console.js"
			},
			"dependencies": [
				"jquery.spinner"
			],
			"messages": [
				"spec-console-title",
				"spec-console-empty",
				"spec-console-cleared",
				"spec-console-run",
				"spec-console-clear"
			]
		},
		"ext.spec.report": {
			"styles": "ext.spec.report.css",
			"position": "top"
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "modules",
		"remoteExtPath": "Spec/modules"
	},
	"TrackingCategories": [
		"spec-tracking-category-good",
		"spec-tracking-category-pending",
		"spec-tracking-category-failing",
		"spec-tracking-category-missing",
		"spec-tracking-category-unknown"
	],
	"SpecialPages": {
	},
	"LogTypes": [
		"track"
	],
	"LogNames": {
		"track": "spec-track-log-name"
	},
	"LogHeaders": {
		"track": "spec-track-log-header"
	},
	"LogActions": {
		"track/good": "spec-track-log-entry-good",
		"track/pending": "spec-track-log-entry-pending",
		"track/failing": "spec-track-log-entry-failing",
		"track/missing": "spec-track-log-entry-missing",
		"track/unknown": "spec-track-log-entry-unknown"
	},
	"manifest_version": 1
}
