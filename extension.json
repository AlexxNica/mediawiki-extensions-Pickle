{
	"name": "Spec",
	"version": "0.1.0",
	"author": [
		"John Erling Blad"
	],
	"url": "https://github.com/jeblad/Spec",
	"descriptionmsg": "spec-desc",
	"license-name": "GPL v2",
	"type": "other",
	"AutoloadClasses": {
		"Spec\\Hooks":
			"includes/Hooks.php",
		"Spec\\Common":
			"includes/Common.php",
		"Spec\\StatusCategory":
			"includes/StatusCategory/StatusCategory.php",
		"Spec\\StatusLog":
			"includes/StatusLog/StatusLog.php",
		"Spec\\StatusIndicator":
			"includes/StatusIndicator/StatusIndicator.php",
		"Spec\\IStrategies":
			"includes/Strategies/IStrategies.php",
		"Spec\\Strategies":
			"includes/Strategies/Strategies.php",
		"Spec\\IExtractStatusStrategy":
			"includes/ExtractStatus/IExtractStatusStrategy.php",
		"Spec\\ExtractStatusDefaultStrategy":
			"includes/ExtractStatus/ExtractStatusDefaultStrategy.php",
		"Spec\\ExtractStatusByPatternStrategy":
			"includes/ExtractStatus/ExtractStatusByPatternStrategy.php",
		"Spec\\ExtractStatusStrategies":
			"includes/ExtractStatus/ExtractStatusStrategies.php",
		"Spec\\IInvokeSubpageStrategy":
			"includes/InvokeSubpage/IInvokeSubpageStrategy.php",
		"Spec\\InvokeSubpageDefaultStrategy":
			"includes/InvokeSubpage/InvokeSubpageDefaultStrategy.php",
		"Spec\\InvokeSubpageByContentTypeStrategy":
			"includes/InvokeSubpage/InvokeSubpageByContentTypeStrategy.php",
		"Spec\\InvokeSubpageStrategies":
			"includes/InvokeSubpage/InvokeSubpageStrategies.php",
		"Spec\\Tests\\StrategiesTestCase":
			"tests/phpunit/includes/Strategies/StrategiesTestCase.php",
		"Spec\\Tests\\InvokeSubpageStrategyTestCase":
			"tests/phpunit/includes/InvokeSubpage/InvokeSubpageStrategyTestCase.php"
	},
	"config": {
		"TrackObserverID": 3,
		"SpecExtractStatus": [
			{
				"class": "Spec\\ExtractStatusByPatternStrategy",
				"name": "failing",
				"pattern": "/\\b(failed|failing|bad|not\\s+ok)\\b/"
			},
			{
				"class": "Spec\\ExtractStatusByPatternStrategy",
				"name": "pending",
				"pattern": "/\\bpending\\b/"
			},
			{
				"class": "Spec\\ExtractStatusByPatternStrategy",
				"name": "good",
				"pattern": "/\\b(good|ok)\\b/"
			}
		],
		"SpecInvokeSubpages": [
			{
				"class": "Spec\\InvokeSubpageByContentTypeStrategy",
				"type": "Scribunto",
				"name": "spec"
			}
		]
	},
	"callback": "Spec\\Hooks::onExtensionSetup",
	"ExtensionMessagesFiles": {
	},
	"Hooks": {
		"ContentAlterParserOutput": [
			"Spec\\Hooks::onContentAlterParserOutput",
			"Spec\\StatusCategory::onContentAlterParserOutput"
		],
		"OutputPageParserOutput": [
			"Spec\\StatusIndicator::onOutputPageParserOutput",
			"Spec\\StatusLog::onOutputPageParserOutput"
		],
		"UnitTestsList": [
			"Spec\\Hooks::onUnitTestsList"
		]
	},
	"MessagesDirs": {
		"Spec": [
			"i18n"
		]
	},
	"ResourceModules": {
		"ext.spec.defaultDisplay": {
			"styles": {
				"0": "ext.spec.defaultDisplay.css"
			}
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "modules",
		"remoteExtPath": "Spec/modules"
	},
	"TrackingCategories": [
		"spec-tracking-category-good",
		"spec-tracking-category-pending",
		"spec-tracking-category-failing",
		"spec-tracking-category-missing",
		"spec-tracking-category-unknown"
	],
	"SpecialPages": {
	},
	"LogTypes": [
		"track"
	],
	"LogNames": {
		"track": "spec-track-log-name"
	},
	"LogHeaders": {
		"track": "spec-track-log-header"
	},
	"LogActions": {
		"track/good": "spec-track-log-entry-good",
		"track/pending": "spec-track-log-entry-pending",
		"track/failing": "spec-track-log-entry-failing",
		"track/missing": "spec-track-log-entry-missing",
		"track/unknown": "spec-track-log-entry-unknown"
	},
	"manifest_version": 1
}
