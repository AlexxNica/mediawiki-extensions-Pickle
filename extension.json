		"Spec\\InvokeSubpageDefault":
			"includes/InvokeSubpage/InvokeSubpageDefault.php",
		"Spec\\InvokeSubpageByContentType":
			"includes/InvokeSubpage/InvokeSubpageByContentType.php",
		"Spec\\InvokeSubpageStrategies":
			"includes/InvokeSubpage/InvokeSubpageStrategies.php",

		"Spec\\IHelpView":
			"includes/Help/IHelpView.php",
		"Spec\\HelpView":
			"includes/Help/HelpView.php",

		"Spec\\IConsoleView":
			"includes/Console/IConsoleView.php",
		"Spec\\ConsoleView":
			"includes/Console/ConsoleView.php",

		"Spec\\ISubLinksView":
			"includes/SubLinks/ISubLinksView.php",
		"Spec\\SubLinksView":
			"includes/SubLinks/SubLinksView.php",

		"Spec\\LuaLibrary":
			"includes/LuaLibrary/LuaLibrary.php",

		"Spec\\Tests\\StrategiesTestCase":
			"tests/phpunit/includes/Strategies/StrategiesTestCase.php",

		"Spec\\Tests\\LogEntryTestCase":
			"tests/phpunit/includes/LogEntry/LogEntryTestCase.php",

		"Spec\\Tests\\CategoryTestCase":
			"tests/phpunit/includes/Category/CategoryTestCase.php",

		"Spec\\Tests\\TAPParserTestCase":
			"tests/phpunit/includes/TAP/TAPParserTestCase.php",

		"Spec\\Tests\\IndicatorTestCase":
			"tests/phpunit/includes/Indicator/IndicatorTestCase.php",

		"Spec\\Tests\\InvokeSubpageTestCase":
			"tests/phpunit/includes/InvokeSubpage/InvokeSubpageTestCase.php"
	},
	"config": {
        "_prefix": "wgSpec",
		"ObserverID": 3,
		"RenderStyles": {
			"compact": "speclib/render/compact",
			"full": "speclib/render/full",
			"vivid": "speclib/render/vivid"
		},
		"RenderTypes": {
			"result": "ResultRenderStrategy",
			"plan": "PlanRenderStrategy"
		},
		"ExtractorStrategy": [
			"speclib/extractor/NilExtractorStrategy",
			"speclib/extractor/FalseExtractorStrategy",
			"speclib/extractor/TrueExtractorStrategy",
			"speclib/extractor/StringExtractorStrategy",
			"speclib/extractor/NumberExtractorStrategy"
		],
		"NeglectPages": [
			"/doc",
			"/conf",
			"/data",
			"/i18n",
			"/l10n"
		],
		"Indicator": [
			{
				"class": "Spec\\IndicatorCommon",
				"name": "fail",
				"icon": "critical-fail"
			},
			{
				"class": "Spec\\IndicatorCommon",
				"name": "todo-fail",
				"icon": "warning-fail"
			},
			{
				"class": "Spec\\IndicatorCommon",
				"name": "todo-good",
				"icon": "warning-good"
			},
			{
				"class": "Spec\\IndicatorCommon",
				"name": "skip-fail",
				"icon": "warning-fail"
			},
			{
				"class": "Spec\\IndicatorCommon",
				"name": "skip-good",
				"icon": "warning-good"
			},
			{
				"class": "Spec\\IndicatorCommon",
				"name": "pending",
				"icon": "warning-unknown"
			},
			{
				"class": "Spec\\IndicatorCommon",
				"name": "good",
				"icon": "information-good"
			},
			{
				"class": "Spec\\IndicatorCommon",
				"name": "missing",
				"icon": "warning-unknown"
			},
			{
				"class": "Spec\\IndicatorDefault",
				"icon": "warning-unknown"
			}
		],
		"Category": [
			{
				"class": "Spec\\CategoryCommon",
				"name": "failing"
			},
			{
				"class": "Spec\\CategoryCommon",
				"name": "pending"
			},
			{
				"class": "Spec\\CategoryCommon",
				"name": "good"
			},
			{
				"class": "Spec\\CategoryCommon",
				"name": "exists"
			},
			{
				"class": "Spec\\CategoryCommon",
				"name": "missing"
			},
			{
				"class": "Spec\\CategoryDefault"
			}
		],
		"LogEntry": [
			{
				"class": "Spec\\LogEntryCommon",
				"name": "failing"
			},
			{
				"class": "Spec\\LogEntryCommon",
				"name": "pending"
			},
			{
				"class": "Spec\\LogEntryCommon",
				"name": "good"
			},
			{
				"class": "Spec\\LogEntryCommon",
				"name": "exists"
			},
			{
				"class": "Spec\\LogEntryCommon",
				"name": "missing"
			},
			{
				"class": "Spec\\LogEntryDefault"
			}
		],
		"InvokeSubpage": [
			{
				"class": "Spec\\InvokeSubpageByContentType",
				"type": "Scribunto",
				"name": "testspec"
			},
			{
				"class": "Spec\\InvokeSubpageByContentType",
				"type": "Scribunto",
				"name": "testcase"
			},
			{
				"class": "Spec\\InvokeSubpageDefault"
			}
		],
		"TAP": [
			{
				"class": "Spec\\TAP13Parser"
			},
			{
				"class": "Spec\\TAPCommonParser"
			}
		],
		"ExtractStatus": [
			{
				"class": "Spec\\ExtractStatusByPattern",
				"name": "fail",
				"pattern": "/^(fail(ing|ed|)|bad|not\\s+ok|bail(ing|ed|)\\s+out!)$/i"
			},
			{
				"class": "Spec\\ExtractStatusByPattern",
				"name": "todo-fail",
				"pattern": "/^(todo)[-_\\s](fail(ing|ed|)|bad|not\\s+ok)$/i"
			},
			{
				"class": "Spec\\ExtractStatusByPattern",
				"name": "todo-good",
				"pattern": "/^(todo)[-_\\s](good|ok)$/i"
			},
			{
				"class": "Spec\\ExtractStatusByPattern",
				"name": "skip-fail",
				"pattern": "/^(skipp?(ing|ed|))[-_\\s](fail(ing|ed|)|bad|not\\s+ok)$/i"
			},
			{
				"class": "Spec\\ExtractStatusByPattern",
				"name": "skip-good",
				"pattern": "/^(skipp?(ing|ed|))[-_\\s](good|ok)$/i"
			},
			{
				"class": "Spec\\ExtractStatusByPattern",
				"name": "pending",
				"pattern": "/^(pending)$/i"
			},
			{
				"class": "Spec\\ExtractStatusByPattern",
				"name": "good",
				"pattern": "/^(good|ok)$/i"
			},
			{
				"class": "Spec\\ExtractStatusDefault"
			}
		]
	},
	"callback": "Spec\\Hooks::onExtensionSetup",
	"ExtensionMessagesFiles": {
	},
	"Hooks": {
		"SpecTesterGadgets": [
			"Spec\\IndicatorFactory::addIndicator"
		],
		"SpecTesteeGadgets": [
			"Spec\\IndicatorFactory::addIndicator",
			"Spec\\CategoryFactory::addCategorization",
			"Spec\\LogEntryFactory::addLogEntry"
		],
		"ContentAlterParserOutput": [
			"Spec\\Hooks::onContentAlterParserOutput"
		],
		"OutputPageParserOutput": [
			"Spec\\SubLinksView::onOutputPageParserOutput"
		],
		"EditPageBeforeEditChecks": [
			"Spec\\ConsoleView::onEditPageBeforeEditChecks"
		],
		"EditPage::showReadOnlyForm:initial": [
			"Spec\\ConsoleView::onShowReadOnlyFormInitial"
		],
		"ScribuntoExternalLibraries": [
			"Spec\\LuaLibrary::registerScribuntoLibraries"
		],
		"ScribuntoExternalLibraryPaths": [
			"Spec\\LuaLibrary::registerScribuntoExternalLibraryPaths"
		],
		"UnitTestsList": [
			"Spec\\Hooks::onUnitTestsList"
		]
	},
	"MessagesDirs": {
		"Spec": [
			"i18n"
		]
	},
	"ResourceModules": {
		"ext.spec.indicator.icon": {
			"position": "left center",
			"class": "ResourceLoaderImageModule",
			"selector": ".mw-speclink-icon-{name}",
			"images": {
				"critical-good": "images/ext.spec.indicator.icon/critical-good.svg",
				"critical-unknown": "images/ext.spec.indicator.icon/critical-unknown.svg",
				"critical-fail": "images/ext.spec.indicator.icon/critical-fail.svg",
				"warning-good": "images/ext.spec.indicator.icon/warning-good.svg",
				"warning-unknown": "images/ext.spec.indicator.icon/warning-unknown.svg",
				"warning-fail": "images/ext.spec.indicator.icon/warning-fail.svg",
				"information-good": "images/ext.spec.indicator.icon/information-good.svg",
				"information-unknown": "images/ext.spec.indicator.icon/information-unknown.svg",
				"information-fail": "images/ext.spec.indicator.icon/information-fail.svg"
			}
		}, 
		"ext.spec.default": {
			"styles": {
				"0": "ext.spec.default.css"
			}
		},
		"ext.spec.console": {
			"styles": {
				"0": "ext.spec.console.css"
			},
			"scripts": {
				"0": "ext.spec.console.js"
			},
			"dependencies": [
				"jquery.spinner"
			],
			"messages": [
				"spec-console-title",
				"spec-console-empty",
				"spec-console-cleared",
				"spec-console-run",
				"spec-console-clear"
			]
		},
		"ext.spec.report": {
			"styles": "ext.spec.report.css",
			"position": "top"
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "modules",
		"remoteExtPath": "Spec/modules"
	},
	"TrackingCategories": [
		"spec-tracking-category-good",
		"spec-tracking-category-pending",
		"spec-tracking-category-failing",
		"spec-tracking-category-missing",
		"spec-tracking-category-unknown"
	],
	"SpecialPages": {
	},
	"LogTypes": [
		"track"
	],
	"LogNames": {
		"track": "spec-track-log-name"
	},
	"LogHeaders": {
		"track": "spec-track-log-header"
	},
	"LogActions": {
		"track/good": "spec-track-log-entry-good",
		"track/pending": "spec-track-log-entry-pending",
		"track/failing": "spec-track-log-entry-failing",
		"track/missing": "spec-track-log-entry-missing",
		"track/unknown": "spec-track-log-entry-unknown"
	},
	"manifest_version": 1
}
