{
	"name": "Spec",
	"version": "0.1.0",
	"author": [
		"John Erling Blad"
	],
	"url": "https://github.com/jeblad/Spec",
	"descriptionmsg": "spec-desc",
	"license-name": "GPL v2",
	"type": "other",
