			"includes/Strategies/Strategies.php",

		"Spec\\TNamedStrategy":
			"includes/Strategies/TNamedStrategy.php",
		"Spec\\TNamedStrategies":
			"includes/Strategies/TNamedStrategies.php",

		"Spec\\ILogEntry":
			"includes/LogEntry/ILogEntry.php",
		"Spec\\LogEntryBase":
			"includes/LogEntry/LogEntryBase.php",
		"Spec\\LogEntryDefault":
			"includes/LogEntry/LogEntryDefault.php",
		"Spec\\LogEntryCommon":
			"includes/LogEntry/LogEntryCommon.php",
		"Spec\\LogEntryFactory":
			"includes/LogEntry/LogEntryFactory.php",

		"Spec\\ICategory":
			"includes/Category/ICategory.php",
		"Spec\\CategoryBase":
			"includes/Category/CategoryBase.php",
		"Spec\\CategoryDefault":
			"includes/Category/CategoryDefault.php",
		"Spec\\CategoryCommon":
			"includes/Category/CategoryCommon.php",
		"Spec\\CategoryFactory":
			"includes/Category/CategoryFactory.php",

		"Spec\\IIndicator":
			"includes/Indicator/IIndicator.php",
		"Spec\\IndicatorBase":
			"includes/Indicator/IndicatorBase.php",
		"Spec\\IndicatorDefault":
			"includes/Indicator/IndicatorDefault.php",
		"Spec\\IndicatorCommon":
			"includes/Indicator/IndicatorCommon.php",
		"Spec\\IndicatorFactory":
			"includes/Indicator/IndicatorFactory.php",

		"Spec\\ITAPParser":
			"includes/TAP/ITAPParser.php",
		"Spec\\TAPBaseParser":
			"includes/TAP/TAPBaseParser.php",
		"Spec\\TAPCommonParser":
			"includes/TAP/TAPCommonParser.php",
		"Spec\\TAP13Parser":
			"includes/TAP/TAP13Parser.php",
		"Spec\\TAPStrategies":
			"includes/TAP/TAPStrategies.php",

		"Spec\\IExtractStatus":
			"includes/ExtractStatus/IExtractStatus.php",
		"Spec\\ExtractStatusDefault":
			"includes/ExtractStatus/ExtractStatusDefault.php",
		"Spec\\ExtractStatusByPattern":
			"includes/ExtractStatus/ExtractStatusByPattern.php",
		"Spec\\ExtractStatusStrategies":
